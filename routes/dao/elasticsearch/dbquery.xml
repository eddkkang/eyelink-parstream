      <query id='getAccTimeseries'><![CDATA[
      {
        "index" : [##index##],  
        "type"  : "access",
        "body" : { 
          "size"   : 10000, 
         "sort" : { "@timestamp" : { "order" : "asc" }},
          "_source" : ["@timestamp", "timestamp", "responsetime", "response"],
           "query" : { 
