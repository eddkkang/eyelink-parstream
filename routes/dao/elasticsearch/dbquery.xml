    <query id='selectOneIndexCount'><![CDATA[
     {
       "index" : #index#,
       "type"  : "access",
       "body" : {
         "size" : 0,
          "aggs": {
            "group_by_time": {
              "range": {
                "field": "responsetime",
                "ranges" : [
                  { "key" : "1s", "to" : "1000" },
                  { "key" : "3s", "from" : "1000", "to" : "3000" },
                  { "key" : "5s", "from" : "3000", "to" : "5000" },
                  { "key" : "slow", "from" : "5000" }
                ],
                "keyed" : true
              },
              "aggs" : {
                "group_by_code" : {
                  "terms" : {   "field": "response"     }
                }
              }
            }
          }
        }
      }
    ]]></query>
    <query id='selectMultiIndexCount'><![CDATA[
     {
       "index" : [##index##],
       "type"  : "access",
       "body" : {
         "size" : 0,
         "aggs" : {
           "group_by_x" : {
             "range": {
               "field": "@timestamp",
               "ranges":  [#range#],
                "keyed" : true
              },
              "aggs": {
                "by_type" : {
                  "range": {
                    "field": "responsetime",
                    "ranges" : [
                      { "key" : "s1", "to" : "1000" },
                      { "key" : "s3", "from" : "1000", "to" : "3000" },
                      { "key" : "s5", "from" : "3000", "to" : "5000" },
                      { "key" : "slow", "from" : "5000" }
                    ],
                    "keyed" : true
                  },
                  "aggs" : {
                    "by_response" : {
                      "range" : {
                        "field": "response",
                        "ranges" : [{ "key" : "cnt", "to" : 400 }]
                      }
                    }
                  }
                },
                "aggs" : {
                  "range" : {
                    "field": "response",
                    "ranges" : [{ "key" : "error", "from" : 400 }]
                  }
                }
              }
            }
          }
        }
      }
    ]]></query>
