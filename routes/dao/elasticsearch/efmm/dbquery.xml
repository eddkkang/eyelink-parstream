<?xml version="1.0"?>
<queryList>
  <dashboard>
    <query id='selectDashboardRawData'><![CDATA[
      {
        "index" : #index#,
        "type"  : #type#,
        "body" : {
          "size" : 100000,
          "sort" : { #sort#: { "order" : "asc" } },
          "query" : {
            "bool": {
              "filter" : {
                "range" : {
                  #sort# : { "gte" : #gte#, "lte" : #lte# }
                }
              }
            }
          }
        }
      }
    ]]></query>
  </dashboard>
  <scheduler>
    <query id='selectApplicationInfo'><![CDATA[
      {
        "index" : #index#,
        "type" : #type#,
        "body" : {
      }
    }]]></query>
    <query id='insertApplicationInfo'><![CDATA[
      {
        body : #BODY#
      }
    ]]></query>
  </scheduler>
</queryList>