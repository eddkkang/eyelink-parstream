<?xml version="1.0"?>
<queryList>
  <dashboard>
    <query id='selectByIndex'><![CDATA[
      {
         "index" : #index#,
         "type" : #type#,
         "body" : {
           "size"   : 10000,
           "query" : {}
         }
      }
    ]]></query>
  </dashboard>
  <timeseries>
    <query id='selectByIndex'><![CDATA[
      {
         "index" : #index#,
         "type" : #type#,
         "body" : {
           "size"   : 10000,
           "query" : {}
         }
      }
    ]]></query>
  </timeseries>  
  <reports>
    <query id='selectJiraAcc'><![CDATA[
      {
        "index" : [##index##],
        "type"  : "access",
        "body" : {
          "size"   : 100000,
          "sort" : { "timestamp" : { "order" : "asc" }},
          "_source" : ["timestamp", "responsetime", "response"],
           "query" : {
             "bool": {
               "must" : {
                 "range" : {
                   "timestamp": { "gte": #gte#, "lte": #lte# }
                 }
              }
            }
          }
        }
      }
    ]]></query>
  </reports>
  <management>
    <query id='selectById'><![CDATA[
      {
        "index" : #INDEX#,
        "type" : #TYPE#,
        "body" : {
          "size" : 10000,
          "query" : {
            "match"  : { #ID# : #VALUE# }
          }
        }
      }
    ]]></query>
  </management>
</queryList>